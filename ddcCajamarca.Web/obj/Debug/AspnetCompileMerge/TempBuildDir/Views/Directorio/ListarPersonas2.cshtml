
@{
    ViewBag.Title = "Directorio";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Directorio Cultural</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Index", "Home")">Inicio</a>
            </li>
            <li>
                Directorio
            </li>
            <li class="active">
                <strong>Directorio Cultural</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">

        @foreach (var item in Model)
        {
            <div class="col-lg-4">
                <div class="contact-box">
                    <a href="#">
                        <div class="col-sm-4">
                            <div class="text-center">
                                <img alt="image" class="img-circle m-t-xs img-responsive" src="@item.Imagen.ToString()">
                                <div class="m-t-xs font-bold">@item.NombreArtistico</div>
                            </div>
                        </div>
                        <div class="col-sm-8">
                            <h3><strong>@item.NombreApellidos</strong></h3>
                            <p><i class="fa fa-id-badge"></i> @item.OcupacionCultural.Nombre</p>
                            <address>
                                <strong>Twitter, Inc.</strong><br>
                                @item.Email<br>
                                @item.Direccion<br>
                                <abbr title="Phone">P:</abbr> @item.Telefono
                            </address>
                        </div>
                        <div class="clearfix"></div>
                    </a>
                    <div class="contact-box-footer">
                        <div class="m-t-xs btn-group">
                            <a class="btn btn-xs btn-white"><i class="fa fa-phone"></i> Call </a>
                            <a class="btn btn-xs btn-white"><i class="fa fa-envelope"></i> Email</a>
                            <a class="btn btn-xs btn-white"><i class="fa fa-user-plus"></i> Follow</a>
                        </div>
                    </div>
                </div>
            </div>
        }

